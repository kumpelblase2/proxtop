{
  "name": "proxtop",
  "version": "1.1.0-beta",
  "description": "A desktop application for proxer.me",
  "main": "src/main/index.js",
  "scripts": {
    "test": "electron-mocha --opts mocha.opts",
    "start": "./run.sh",
    "dist": "make package",
    "debug": "electron --debug ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kumpelblase2/proxtop.git"
  },
  "keywords": [
    "electron",
    "desktop",
    "anime",
    "proxer"
  ],
  "author": "kumpelblase2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumpelblase2/proxtop/issues"
  },
  "homepage": "https://github.com/kumpelblase2/proxtop#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "devtron": "1.4.0",
    "electron": "1.7.9",
    "electron-builder": "19.45.5",
    "electron-builder-lib": "19.46.1",
    "electron-builder-squirrel-windows": "19.46.0",
    "electron-mocha": "3.4.0",
    "electron-packager": "9.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "about-window": "1.6.1",
    "bluebird": "3.4.6",
    "cheerio": "0.22.0",
    "electron-window-state": "4.0.0",
    "lodash": "4.17.2",
    "lowdb": "0.16.0",
    "moment": "2.17.1",
    "opn": "^5.2.0",
    "request": "2.79.0",
    "request-promise": "4.1.1",
    "semver": "5.3.0",
    "tough-cookie": "2.3.2",
    "winston": "2.3.0"
  },
  "build": {
    "appId": "de.eternalwings.proxtop",
    "mac": {
      "category": "public.app-category.entertainment",
      "target": [
        "zip",
        "tar.gz",
        "dmg"
      ]
    },
    "win": {
      "target": [
        "squirrel",
        "zip"
      ]
    },
    "directories": {
      "app": "_dist",
      "output": "_packaged"
    }
  }
}
