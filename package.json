{
  "name": "proxtop",
  "version": "1.1.0-beta",
  "description": "A desktop application for proxer.me",
  "main": "dist/index.js",
  "scripts": {
    "test": "electron-mocha --opts mocha.opts",
    "start": "./run.sh",
    "dist": "webpack-cli --mode=production",
    "build": "electron-builder",
    "build:all": "electron-builder -mwl",
    "build:windows": "electron-builder -w --x64 --ia32",
    "build:osx": "electron-builder -m",
    "build:linux": "electron-builder -l",
    "debug": "electron --inspect=5858 dist",
    "webpack": "webpack-cli --mode=development --watch",
    "clean": "(test -d _packaged && rm -r _packaged) || (test -d dist && rm -r dist)"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kumpelblase2/proxtop.git"
  },
  "keywords": [
    "electron",
    "desktop",
    "anime",
    "proxer"
  ],
  "author": "kumpelblase2",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumpelblase2/proxtop/issues"
  },
  "homepage": "https://github.com/kumpelblase2/proxtop#readme",
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "css-loader": "1.0.0",
    "devtron": "1.4.0",
    "electron": "15.5.5",
    "electron-builder": "20.28.4",
    "electron-builder-lib": "20.23.1",
    "electron-builder-squirrel-windows": "20.28.3",
    "electron-mocha": "8.1.2",
    "electron-packager": "14.0.6",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "0.23.0",
    "ts-loader": "5.1.1",
    "typescript": "3.0.3",
    "url-loader": "1.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.0",
    "@types/bluebird": "^3.5.20",
    "@types/electron": "^1.6.10",
    "@types/lodash": "4.14.116",
    "@types/moment": "2.13.0",
    "@types/request-promise": "^4.1.41",
    "@types/semver": "5.5.0",
    "@types/winston": "2.4.4"
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.20",
    "about-window": "1.6.1",
    "angular": "1.7.9",
    "angular-animate": "1.7.9",
    "angular-aria": "1.7.9",
    "angular-debounce": "1.1.0",
    "angular-material": "1.1.21",
    "angular-messages": "1.7.9",
    "angular-progress-arc": "1.0.0",
    "angular-sanitize": "1.7.9",
    "angular-translate": "2.18.2",
    "angular-ui-router": "1.0.24",
    "bluebird": "3.4.6",
    "cheerio": "0.22.0",
    "electron-window-state": "5.0.3",
    "font-awesome": "4.7.0",
    "lodash": "4.17.15",
    "lowdb": "1.0.0",
    "moment": "2.22.2",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "semver": "5.5.1",
    "tough-cookie": "2.4.3",
    "winston": "3.2.1"
  },
  "build": {
    "appId": "de.eternalwings.proxtop",
    "copyright": "Copyright Â© 2014-2019 Tim Hagemann",
    "files": [
      "dist/**/*",
      "static/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.entertainment",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "perMachine": true
    },
    "linux": {
      "maintainer": "Tim Hagemann <tim+github at eternalwings.de>",
      "synopsis": "A Proxer.me desktop client",
      "desktop": "proxtop.desktop",
      "target": [
        "deb",
        "pacman",
        "tar.gz"
      ]
    },
    "publish": null,
    "directories": {
      "buildResources": "build",
      "output": "_packaged"
    }
  }
}
